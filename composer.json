{
    "name": "foxliscom/geo",
    "description": "Get user location by IP using Foxlis Geo API",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Sorokin",
            "email": "astitrance@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.2",
        "composer-plugin-api": "^1.0 || ^2.0"
    },
    "require-dev": {
        "composer/composer": "^1.0 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "Foxliscom\\Geo\\": "/src"
        },
        "files": ["src/Config/common.php"]
    },
    "extra": {
        "class": "Foxliscom\\Geo\\FoxlisGeo"
    }
}
